<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.znvip.nicecomponent">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_nice"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_nice_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".entry.NicePopActivity"
            android:label="NicePop" />
        <activity
            android:name=".entry.NiceBottomSheetActivity"
            android:label="NiceBottomSheet" />

    </application>

</manifest>